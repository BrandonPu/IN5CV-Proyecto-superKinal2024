<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="org.brandonpu.controller.FormUsuarioController">
   <children>
      <ImageView fitHeight="700.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/LoginUsuario.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfUser" layoutX="122.0" layoutY="181.0" prefHeight="40.0" prefWidth="300.0" />
      <ComboBox fx:id="cmbEmpleado" layoutX="122.0" layoutY="400.0" prefHeight="40.0" prefWidth="300.0" />
      <Button fx:id="btnRegistrar" layoutX="370.0" layoutY="605.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="50.0" prefWidth="104.0" text="Registrar" />
      <TextField fx:id="tfPassword" layoutX="122.0" layoutY="283.0" prefHeight="40.0" prefWidth="300.0" />
      <ComboBox fx:id="cmbNivelAcceso" layoutX="122.0" layoutY="518.0" prefHeight="40.0" prefWidth="300.0" />
      <Button fx:id="btnEmpleado" layoutX="70.0" layoutY="605.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="50.0" prefWidth="104.0" text="Agregar Empleado" textAlignment="CENTER" wrapText="true" />
   </children>
</AnchorPane>
